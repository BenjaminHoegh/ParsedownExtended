{
  "name": "benjaminhoegh/parsedown-extended",
  "description": "An extension for Parsedown.",
  "keywords": [
    "markdown",
    "markdown extended",
    "parsedown",
    "parsedown extended"
  ],
  "homepage": "https://github.com/benjaminhoegh/parsedown-extended",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Benjamin Hoegh",
      "homepage": "https://github.com/benjaminhoegh"
    }
  ],
  "require": {
    "php": ">=7.4",
    "erusev/parsedown": ">=1.7.4 <2.0",
    "erusev/parsedown-extra": ">=0.8.1"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-0": {
      "ParsedownExtended": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "vimeo/psalm": "^5.22"
  }
}
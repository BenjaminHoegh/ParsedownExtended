{
    "name": "benjaminhoegh/parsedown-extreme",
    "description": "An extension of Parsedown.",
    "keywords": ["markdown", "markdown extra", "parser", "parsedown"],
    "homepage": "https://github.com/benjaminhoegh/parsedown-extreme",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Benjamin Hoegh",
            "homepage": "https://github.com/benjaminhoegh"
        }
    ],
    "require": {
        "erusev/parsedown": "^1.8.0-beta-5",
        "erusev/parsedown-extra": "^0.8.0-beta-1",
        "php": ">=5.6",
        "ext-dom": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.35"
    },
    "autoload": {
        "psr-0": {"parsedown-extreme": ""}
    },
    "autoload-dev": {
        "psr-0": {
            "TestParsedown": "test/",
            "ParsedownExtraTest": "test/",
            "ParsedownTest": "vendor/benjaminhoegh/parsedown/test/",
            "CommonMarkTest": "vendor/benjaminhoegh/parsedown/test/",
            "CommonMarkTestWeak": "vendor/benjaminhoegh/parsedown/test/"
        }
    }
}
